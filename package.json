{
  "name": "cryptoauthsystem-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "gui"
  ],
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "dev:backend": "npm run dev",
    "dev:frontend": "npm run dev -w gui",
    "dev:all": "concurrently \"npm:dev:backend\" \"npm:dev:frontend\""
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^11.0.1",
    "nodemailer": "^7.0.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.11.1"
  }
}
